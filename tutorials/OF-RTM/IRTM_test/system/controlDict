FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     compressibleFiberPorousInterFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         10;

fullValue		0.995;

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo		0.3;
maxAlphaCo	0.3;

maxDeltaT	1;

libs 	(
		"libsampling.so"	
        );
// ************************************************************************* //
