FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions	[-1 1 1 0 0 0 0];

internalField	uniform 0;


boundaryField
{
	inlet
	{
		type	        	zeroGradient;
	}

	
	outlet
	{
		type	        	zeroGradient;
	}

	    leftWall
	    {
		type	        	zeroGradient;
	    }
	    rightWall
	    {
		type	        	zeroGradient;
	    }

	    lowerWall
	    {
		type	        	zeroGradient;
	    }
	    upperWall
	    {
		type	        	zeroGradient;
	    }    
}


