FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue H 0.01
);

regions
(
	boxToCell
	{
		box (0.02 0.01 -1) (0.08 0.19 1);
        fieldValues
        (
            volScalarFieldValue H 0.002
        );
	}

);


