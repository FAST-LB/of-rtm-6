FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     eikonalFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         20000;

fullValue		0.995;

deltaT          1;

writeControl    adjustableRunTime;

writeInterval   20000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo		0.3;
maxAlphaCo	0.3;

maxDeltaT	1;

libs 	(
		"libsampling.so"	
        );
